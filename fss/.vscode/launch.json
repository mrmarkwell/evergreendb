{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Main Electron Process",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": true,
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd",
            "runtimeArgs": [
                "${workspaceRoot}/.",
                "--remote-debugging-port=9222"
            ],
            "webRoot": "${workspaceRoot}/dist",
            "skipFiles": [
                "node_modules/**/*.js"
            ],
            "preLaunchTask": "build",
        }
    ],
    "compounds": []
}