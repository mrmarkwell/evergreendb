{
  "name": "fss",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prodbuild": "ng build --prod",
    "build": "ng build",
    "watch": "ng build --watch",
    "fastwatch": "ng build --watch --sourcemap=false",
    "packagelocal": "npm run prodbuild && electron-packager . fss --out=release --asar --overwrite",
    "packagewin32": "npm run prodbuild && electron-packager . fss --out=release --asar --overwrite --platform=win32 --arch=ia32",
    "packageall": "npm run prodbuild && electron-packager . fss --out=release --asar --overwrite --all",
    "fastbuild": "ng build --sourcemap=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "superslowstart": "npm run prodbuild && npm run electron",
    "slowstart": "npm run build && npm run electron",
    "faststart": "npm run fastbuild && npm run electron"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.3",
    "@angular/cdk": "^5.0.0-rc.1",
    "@angular/common": "^5.0.3",
    "@angular/compiler": "^5.0.3",
    "@angular/core": "^5.0.3",
    "@angular/forms": "^5.0.3",
    "@angular/http": "^5.0.3",
    "@angular/material": "^5.0.0-rc.1",
    "@angular/platform-browser": "^5.0.3",
    "@angular/platform-browser-dynamic": "^5.0.3",
    "@angular/router": "^5.0.3",
    "core-js": "^2.4.1",
    "ng2-file-upload": "^1.2.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.4",
    "@angular/compiler-cli": "^5.0.3",
    "@angular/language-service": "^5.0.3",
    "@angular/material-moment-adapter": "^5.1.1",
    "@types/jasmine": "~2.8.2",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~8.0.26",
    "codelyzer": "~4.0.1",
    "devtron": "^1.4.0",
    "electron": "^1.8.2-beta.5",
    "electron-debug": "^1.4.0",
    "electron-packager": "^10.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "moment": "^2.20.1",
    "node-sass": "^4.5.3",
    "protractor": "~5.2.0",
    "semaphore": "^1.1.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "2.4.2"
  }
}
